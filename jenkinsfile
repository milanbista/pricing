pipeline{
  agent any

  stages{
    stage('checkout'){
      steps{
        git branch: 'main', url: 'https://github.com/milanbista/pricing.git'
      }

    }

    stage('build'){
      steps{
          sh '''
          echo "Building Pricing Service"
          ./gradlew clean build
          '''
      }
    }


  stage('package'){
    steps{
      sh '''
      cat /buld/libs
      '''
    }

  }
  }
}
